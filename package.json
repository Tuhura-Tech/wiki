{
  "name": "wiki",
  "version": "0.1.0",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "preview:cloudflare": "wrangler pages dev ./dist",
    "check": "astro check",
    "format": "prettier -w . --cache --plugin-search-dir=.",
    "lint:eslint": "eslint .",
    "old": "echo bad",
    "docgen": "node ./scripts/docgen.mjs",
    "docgen:errors": "node ./scripts/error-docgen.mjs",
    "lint:a11y": "start-test 'yarn build && yarn preview' 3000 'yarn lint:a11y:local'",
    "lint:linkcheck": "SKIP_OG=true pnpm build && tsm --require=./scripts/lib/filter-warnings.cjs ./scripts/lint-linkcheck.ts",
    "lint:linkcheck:nobuild": "tsm --require=./scripts/lib/filter-warnings.cjs ./scripts/lint-linkcheck.ts",
    "lint:slugcheck": "node ./scripts/lint-slugcheck.mjs",
    "netlify:build": "pnpm ${NETLIFY_BUILD_SCRIPT:-build}",
    "lunaria:build": "tsm ./scripts/lunaria.mts"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/cloudflare": "^12.0.0",
    "@astrojs/sitemap": "^3.2.1",
    "@astrojs/starlight": "^0.29.0",
    "@astrojs/starlight-docsearch": "^0.3.0",
    "@astrojs/starlight-tailwind": "^2.0.1",
    "@astrojs/tailwind": "^5.1.0",
    "@docsearch/js": "^3.5.2",
    "@expressive-code/plugin-collapsible-sections": "^0.38.3",
    "@lunariajs/core": "https://pkg.pr.new/lunariajs/lunaria/@lunariajs/core@62a858f",
    "astro": "^4.0.8",
    "canvas-confetti": "^1.6.0",
    "jsdoc-api": "^9.3.4",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-mathjax": "^6.0.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0",
    "remark-smartypants": "^2.0.0",
    "sharp": "^0.33.1",
    "starlight-image-zoom": "^0.9.0",
    "starlight-links-validator": "^0.13.0",
    "tailwindcss": "^3.4.0"
  },
  "devDependencies": {
    "@11ty/eleventy-fetch": "^3.0.0",
    "@actions/core": "^1.9.0",
